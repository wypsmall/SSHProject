<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd" default-autowire="byName">
	
	<!-- 登录日志 -->
	<bean name="/sysmgr/loginlog" class="com.imti.sysmgr.action.LoginLogAction" />
	<!-- 资源（模块） -->
	<bean name="/sysmgr/resources" class="com.imti.sysmgr.action.SysResourceAction" />
	<!-- 系统管理 -->
	<bean name="/sysmgr/sysmgr" class="com.imti.sysmgr.action.SysMgrAction" />
	<!-- 错误信息 -->
	<bean name="/sysmgr/warn" class="com.imti.sysmgr.action.WarnAction" />
</beans>