<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.yzsystem.tperson.po.TPersonPO" table="T_PERSON">
        <id name="personId" column="PERSONID" type="java.lang.Integer">
            <generator class="native"/>
        </id>
		<property name="personName" type="string" column="PERSONNAME" />
		<property name="inTime" type="string" column="INTIME" />
		<property name="modTime" type="string" column="MODTIME" />
    </class>
</hibernate-mapping>