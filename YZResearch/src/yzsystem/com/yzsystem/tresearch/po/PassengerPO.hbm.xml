<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.yzsystem.tresearch.po.PassengerPO" table="T_PASSENGER">
        <id name="uid" column="uid" type="java.lang.Integer">
            <generator class="native"/>
        </id>
		<property name="uname" type="string" column="uname" />
		<property name="usex" type="java.lang.Integer" column="usex" />
		<property name="ucompany" type="string" column="ucompany" />
		<property name="uroom" type="string" column="uroom" />
		<property name="inTime" type="string" column="intime" />
		<property name="modTime" type="string" column="MODTIME" />
    </class>
</hibernate-mapping>